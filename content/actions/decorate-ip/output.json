{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "GeoData",
  "type": "object",
  "properties": {
    "geo": {
      "type": "object",
      "properties": {
        "country": {
          "type": ["string", "null"]
        },
        "state": {
          "type": ["string", "null"]
        },
        "city": {
          "type": ["string", "null"]
        }
      },
      "required": ["country", "state", "city"]
    }
  },
  "asn": {
    "type": "object",
    "properties": {
      "org": {
        "type": ["string", "null"]
      },
      "num": {
        "type": ["string", "null"]
      }
    }
  }
}

